<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/court_sub_table_layout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    
     android:gravity="left"
    android:paddingLeft="10dip"
    android:paddingRight="10dip" >

    <TableRow android:gravity="left" >

       <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
            android:weightSum="10" >
   <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
             <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
              
                android:text="New player adult program:"
                android:textColor="#fff"
                android:textSize="10dp"
                android:textStyle="normal"
                android:typeface="serif" >
            </TextView></LinearLayout>
              <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/adultProgram"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" >
            </ImageView>
</LinearLayout>
          
        </LinearLayout>
</TableRow>
       <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
          
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
                 <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                android:text="Team tennis:"
                android:textColor="#fff"
                android:textSize="10dp"
                android:textStyle="normal"
                android:typeface="serif" >
            </TextView></LinearLayout>
                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/teamTennis"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" >
            </ImageView>
</LinearLayout>
           
        </LinearLayout>
    </TableRow>

     <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
            android:weightSum="10" >
    <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
              
                android:text="New player junior program:"
                android:textColor="#fff"
                android:textSize="10dp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
   <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/juniorProgram"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" /></LinearLayout>

        
        </LinearLayout></TableRow>
 <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
          
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
      
    <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                
                android:text="Tournaments:"
                android:textColor="#fff"
                android:textSize="10sp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
             <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/tournaments"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" /></LinearLayout>

        
        </LinearLayout>
    </TableRow>

    <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
           
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >

 <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
              
                android:text="Ladders:"
                android:textColor="#fff"
                android:textSize="10dp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
   <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
                 
            <ImageView
                android:id="@+id/ladders"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" /></LinearLayout>

           
        </LinearLayout>
        </TableRow>
        
  <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
           
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
    
    <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
               android:gravity="center_vertical"
              
                android:text="Round robins:"
                android:textColor="#fff"
                android:textSize="10sp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
              <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/roundRobin"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" />
</LinearLayout>
        
        </LinearLayout>
    </TableRow>

    <TableRow android:gravity="left" >

       <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
           
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
     <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                
                android:text="Social/Mixers:"
                android:textColor="#fff"
                android:textSize="10dp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
              <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            <ImageView
                android:id="@+id/socialMixers"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" /></LinearLayout>

       
        </LinearLayout>
        </TableRow>
   <TableRow android:gravity="left" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="left"
            android:orientation="horizontal"
           
            android:weightSum="10" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.5"
                android:orientation="horizontal" >
           <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
              
                android:text="Seniors 50+:"
                android:textColor="#fff"
                android:textSize="10sp"
                android:textStyle="normal"
                android:typeface="serif" /></LinearLayout>
                <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8.5"
                android:orientation="horizontal" >
            
            <ImageView
                android:id="@+id/seniors"
                android:layout_width="15dp"
                android:layout_height="wrap_content"
                android:src="@drawable/check_or_uncheck_image" /></LinearLayout>

      
        </LinearLayout>
    </TableRow>

</TableLayout>